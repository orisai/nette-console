{
	"name": "orisai/nette-console",
	"description": "Symfony Console integration for Nette",
	"keywords": [
		"console",
		"cli",
		"nette",
		"orisai",
		"symfony"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-console",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"nette/php-generator": "^3.6.0",
		"nette/utils": "^3.1.3",
		"orisai/nette-di": "^1.0.0",
		"symfony/console": "^5.3.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.25.0",
		"orisai/coding-standard": "^2.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^9.3.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.1.0",
		"symfony/event-dispatcher": "^5.3.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Console\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\Console\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	}
}
